#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/react-clear-cache@1.4.14_react-dom@17.0.2+react@17.0.2/node_modules/react-clear-cache/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/react-clear-cache@1.4.14_react-dom@17.0.2+react@17.0.2/node_modules/react-clear-cache/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/react-clear-cache@1.4.14_react-dom@17.0.2+react@17.0.2/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/react-clear-cache/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/react-clear-cache/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/react-clear-cache@1.4.14_react-dom@17.0.2+react@17.0.2/node_modules/react-clear-cache/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/react-clear-cache@1.4.14_react-dom@17.0.2+react@17.0.2/node_modules/react-clear-cache/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/react-clear-cache@1.4.14_react-dom@17.0.2+react@17.0.2/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/react-clear-cache/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/react-clear-cache/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-clear-cache/bin/cli.js" "$@"
else
  exec node  "$basedir/../react-clear-cache/bin/cli.js" "$@"
fi
