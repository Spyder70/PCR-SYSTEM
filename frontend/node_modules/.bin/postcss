#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/postcss-cli@10.1.0_postcss@8.4.31/node_modules/postcss-cli/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/postcss-cli@10.1.0_postcss@8.4.31/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/postcss-cli/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/postcss-cli@10.1.0_postcss@8.4.31/node_modules/postcss-cli/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/postcss-cli@10.1.0_postcss@8.4.31/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/postcss-cli/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../postcss-cli/index.js" "$@"
else
  exec node  "$basedir/../postcss-cli/index.js" "$@"
fi
