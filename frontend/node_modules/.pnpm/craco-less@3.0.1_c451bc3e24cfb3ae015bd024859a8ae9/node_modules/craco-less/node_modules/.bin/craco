#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules/@craco/craco/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules/@craco/craco/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules/@craco/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/craco-less@3.0.1_c451bc3e24cfb3ae015bd024859a8ae9/node_modules/craco-less/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/craco-less@3.0.1_c451bc3e24cfb3ae015bd024859a8ae9/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules/@craco/craco/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules/@craco/craco/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules/@craco/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/@craco+craco@6.4.5_react-scripts@5.0.1/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/craco-less@3.0.1_c451bc3e24cfb3ae015bd024859a8ae9/node_modules/craco-less/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/craco-less@3.0.1_c451bc3e24cfb3ae015bd024859a8ae9/node_modules"
