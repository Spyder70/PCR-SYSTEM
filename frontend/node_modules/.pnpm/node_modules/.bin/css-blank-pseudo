#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.31/node_modules/css-blank-pseudo/dist/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.31/node_modules/css-blank-pseudo/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.31/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules/css-blank-pseudo/dist/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules/css-blank-pseudo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.31/node_modules/css-blank-pseudo/dist/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.31/node_modules/css-blank-pseudo/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.31/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules/css-blank-pseudo/dist/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/frontend/node_modules/.pnpm/node_modules/css-blank-pseudo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-blank-pseudo/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../css-blank-pseudo/dist/cli.cjs" "$@"
fi
