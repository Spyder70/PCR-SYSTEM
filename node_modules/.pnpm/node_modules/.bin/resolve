#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/node_modules/resolve/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/node_modules/resolve/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules:/mnt/c/Users/Spyder/Desktop/node_modules:/mnt/c/Users/Spyder/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/node_modules/resolve/bin/node_modules:/mnt/c/Users/Spyder/Desktop/pcr-system-clone/node_modules/.pnpm/node_modules/resolve/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../resolve/bin/resolve" "$@"
fi
